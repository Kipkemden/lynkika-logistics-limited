{
  "name": "lynkika-logistics",
  "version": "1.0.0",
  "description": "Enterprise-grade logistics operations platform",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "build": "npm run install-client && npm run build-client",
    "start": "node server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "logs:view": "tail -f logs/combined.log",
    "logs:errors": "tail -f logs/error.log",
    "logs:security": "tail -f logs/security.log",
    "cache:clear": "node -e \"require('./middleware/cache').clearAllCaches()\"",
    "health:check": "curl -f http://localhost:5000/health || exit 1",
    "deploy:staging": "npm run build && npm run test && echo 'Ready for staging deployment'",
    "deploy:production": "npm run security:audit && npm run build && npm run test && echo 'Ready for production deployment'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.87.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-winston": "^4.2.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.1",
    "node-cache": "^5.1.2",
    "serverless-http": "^3.2.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "logistics",
    "enterprise",
    "material-design"
  ],
  "author": "Lynkika Logistics",
  "license": "PROPRIETARY",
  "engines": {
    "node": "22.x"
  }
}
